{
  "private": true,
  "sideEffects": false,
  "license": "AGPL-3.0",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev",
    "deploy": "flyctl deploy --remote-only",
    "import-env": "node fly_import_env.js --env-file .env",
    "dev:staging": "cross-env STAGING=true remix dev",
    "start": "remix-serve ./build/index.js",
    "start:staging": "cross-env STAGING=true remix-serve ./build/index.js",
    "typecheck": "tsc",
    "lint": "eslint app",
    "lint:fix": "eslint app --fix",
    "lint:tsc": "tsc --noEmit",
    "format": "prettier --check --ignore-unknown ./app ./public",
    "format:fix": "prettier --write --ignore-unknown ./app ./public",
    "knip": "knip"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@icons-pack/react-simple-icons": "^10.2.0",
    "@nivo/sankey": "^0.88.0",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@reduxjs/toolkit": "^2.5.0",
    "@remix-run/node": "^2.15.2",
    "@remix-run/react": "^2.15.2",
    "@remix-run/serve": "^2.15.2",
    "@remix-run/v1-route-convention": "^0.1.4",
    "@swetrix/sdk": "^1.1.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.0.0",
    "@types/ua-parser-js": "^0.7.39",
    "axios": "^1.7.9",
    "axios-auth-refresh": "^3.3.6",
    "billboard.js": "^3.14.2",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "dangerously-set-html-content": "^1.1.0",
    "dayjs": "^1.11.13",
    "file-saver": "^2.0.5",
    "flatpickr": "^4.6.13",
    "framer-motion": "^11.15.0",
    "i18n-iso-countries": "^7.13.0",
    "i18next": "^23.16.8",
    "i18next-browser-languagedetector": "^8.0.2",
    "i18next-fs-backend": "^2.6.0",
    "i18next-http-backend": "^3.0.1",
    "isbot": "^5.1.19",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.469.0",
    "marked": "^15.0.4",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-flatpickr": "^3.10.13",
    "react-hotkeys-hook": "^4.6.1",
    "react-i18next": "^15.3.0",
    "react-outside-click-handler": "^1.3.0",
    "react-qr-code": "^2.0.15",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "remix-i18next": "^6.4.1",
    "remix-sitemap": "^3.2.0",
    "remix-utils": "^7.7.0",
    "sonner": "^1.7.1",
    "spacetime": "^7.7.0",
    "swetrix": "^3.5.3",
    "timezone-soft": "^1.5.2",
    "ua-parser-js": "^2.0.0"
  },
  "eslintConfig": {
    "extends": [
      "@remix-run/eslint-config",
      "eslint:recommended"
    ],
    "globals": {
      "JSX": "readonly"
    },
    "rules": {
      "jsx-quotes": [
        "error",
        "prefer-single"
      ],
      "comma-dangle": [
        "warn",
        "always-multiline"
      ],
      "@typescript-eslint/consistent-type-imports": "off",
      "jsx-a11y/anchor-has-content": "off",
      "no-unused-vars": "off",
      "semi": [
        "error",
        "never"
      ]
    },
    "settings": {
      "import/resolver": {
        "node": {
          "moduleDirectory": [
            "node_modules",
            "app"
          ],
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        }
      }
    },
    "env": {
      "browser": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@remix-run/dev": "^2.15.2",
    "@remix-run/eslint-config": "^2.15.2",
    "@tailwindcss/typography": "^0.5.16",
    "@types/file-saver": "^2.0.7",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-flatpickr": "^3.8.11",
    "@types/react-outside-click-handler": "^1.3.4",
    "@typescript-eslint/parser": "^8.18.2",
    "@typescript-eslint/typescript-estree": "^8.18.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "knip": "^5.41.1",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "onFail": "error",
      "version": ">=20"
    }
  }
}
